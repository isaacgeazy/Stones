            padding: 10px;
            text-decoration: none;
            cursor: pointer;
            transition: background .3s;
            border-radius: 5px;
        }

        .btn-sucess {
            background-color: var(--cor-primaria-sucess);
            outline: none;
            color: var(--cor-secundaria-sucess)
        }

        .btn-sucess:hover {
            background-color: var(--cor-hover-sucess);
        }

        .btn-sucess:active {
            color: var(--cor-primaria-success);
            background-color: var(--cor-secundaria-sucess);
        }
